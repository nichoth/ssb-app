{
  "name": "ssb-app",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server\"  \"npm run budo\" \"npm run sass-watch\"",
    "server": "node ./src/server.js",
    "budo": "env $(cat .env | xargs) budo src/index.js:bundle.js --pushstate --dir=public --live -- `cat scripts/browserify-args-dev | tr -d '\n'`",
    "sass-watch": "node-sass --watch src/style/main.scss --source-map-embed public/style.css",
    "test": "set -e; for t in test/*.js; do echo TEST $t; node $t; done"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "muxrpc": "^6.4.2",
    "pull-stream": "^3.6.14",
    "pull-ws": "^3.3.2",
    "ssb-backlinks": "^0.7.3",
    "ssb-config": "^3.3.2",
    "ssb-gossip": "^1.1.1",
    "ssb-master": "^1.0.3",
    "ssb-replicate": "^1.3.0",
    "ssb-server": "^15.1.1"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "bundle-collapser": "^1.3.0",
    "concurrently": "^4.1.2",
    "envify": "^4.1.0",
    "eslint": "^6.4.0",
    "eslint-config-minlint": "^2.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "getport": "^0.1.0",
    "node-sass": "^7.0.0",
    "tape": "^4.11.0",
    "uglify-js": "^3.6.0",
    "unassertify": "^2.1.1"
  }
}
